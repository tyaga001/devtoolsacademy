{
  "name": "dev-tools-compare",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "typecheck": "tsc --noEmit",

    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:validate": "prisma validate",
    "db:format": "prisma format",

    "format": "prettier --write --ignore-path .gitignore .",
    "format:check": "prettier --check --ignore-path .gitignore .",

    "download-logos": "ts-node --transpile-only scripts/download-logos.ts",

    "setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "dev:seed": "npm run db:reset && npm run dev",
    "build:full": "npm run clean && npm run db:generate && next build",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "seed:blog": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/seed-blog.ts",
    "db:seed:blog": "npm run db:generate && ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seeds/blog.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.2.4",
    "@heroicons/react": "^2.1.5",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@neondatabase/serverless": "^0.9.4",
    "@next/mdx": "^14.2.7",
    "@prisma/client": "^5.21.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.3.2",
    "@vercel/speed-insights": "^1.0.12",
    "axios": "^1.7.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "geist": "^1.3.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.10.0",
    "lucide-react": "^0.416.0",
    "mermaid": "^11.0.2",
    "next": "^14.2.13",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.3.0",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.13.1",
    "react-type-animation": "^3.2.0",
    "rehype-highlight": "^7.0.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.17.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/tailwindcss": "^3.1.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
